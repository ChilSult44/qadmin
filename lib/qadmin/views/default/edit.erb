<h2><%= model_human_name %>: Edit</h2>

<%= admin_controls model_collection_name, :for => :edit, :object => @model_instance %>

<%= error_messages_for model_instance_name %>

<% form_for(@model_instance, :url => { :action => :update }, :html => {:multipart => true, :method => :put }) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f, model_instance_name.to_sym => @model_instance} %>
  <p><%= f.submit "Update" %></p>
<% end %>

<%= admin_controls model_collection_name, :for => :edit, :object => @model_instance %>
